{
  "name": "ghost-chat",
  "version": "2.0.0",
  "private": true,
  "description": "Standalone twitch chat overlay",
  "author": {
    "name": "Marco Schuster"
  },
  "main": "dist-electron/main/index.js",
  "license": "MIT",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
