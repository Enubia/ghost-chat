{
    "git": {
        "commitMessage": "chore: release v${version}"
    },
    "npm": {
        "publish": false
    },
    "hooks": {
        "after:version:bump": "auto-changelog -p -l false -t ./changelog-template.hbs --ignore-commit-pattern \"chore\\(deps\\)|chore: release|Enubia/renovate\" --starting-version v2.0.0"
    }
}
